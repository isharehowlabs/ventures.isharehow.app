# Rewrite API requests to PHP proxy
<IfModule mod_rewrite.c>
    RewriteEngine On
    
    # Removed redirect from /live to /labs - /live should work independently
    
    # Rewrite API requests
    RewriteCond %{REQUEST_URI} ^/api/products
    RewriteRule ^api/products$ api/products.php [L]
</IfModule>

