    <title>PACT Venture - Empowering Communities Through Property Care Jobs</title>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;700&display=swap" rel="stylesheet">
    <style>
        body {
            font-family: 'Montserrat', Arial, sans-serif;
            margin: 0;
            padding: 0;
            background: linear-gradient(135deg, #2c5aa0 0%, #1e2a47 100%);
            color: #f4f4f4;
        }
        header {
            position: relative;
            background: linear-gradient(90deg, #2c5aa0 60%, #1e2a47 100%);
            color: #fff;
            padding: 40px 20px 20px 20px;
            text-align: left;
            display: flex;
            align-items: center;
            gap: 30px;
            box-shadow: 0 4px 16px rgba(44,90,160,0.15);
        }
        .header-logo {
            width: 80px;
            height: 80px;
            border-radius: 50%;
            box-shadow: 0 2px 8px rgba(44,90,160,0.25);
            background: #fff;
            object-fit: cover;
        }
        .header-content {
            flex: 1;
        }
        .logo {
            font-size: 2.2em;
            font-weight: bold;
            color: #ffd700;
            letter-spacing: 2px;
            margin-bottom: 8px;
            font-family: 'Montserrat', Arial, sans-serif;
        }
        .header-tagline {
            font-size: 1.1em;
            color: #e0e0e0;
            font-weight: 500;
        }
        nav {
            background: #1e2a47;
            padding: 12px 0;
            text-align: center;
            box-shadow: 0 2px 8px rgba(44,90,160,0.10);
        }
        nav a {
            color: #ffd700;
            margin: 0 18px;
            text-decoration: none;
            font-weight: bold;
            font-size: 1.08em;
            transition: color 0.2s;
        }
        nav a:hover {
            color: #ff6b35;
        }
        .hero {
            background: linear-gradient(120deg, #2c5aa0 60%, #1e2a47 100%);
            color: #fff;
            text-align: center;
            padding: 90px 20px 60px 20px;
            box-shadow: 0 2px 12px rgba(44,90,160,0.10);
        }
        .hero h1 {
            font-size: 2.8em;
            margin-bottom: 18px;
            font-weight: 700;
            color: #ffd700;
            text-shadow: 1px 2px 8px #1e2a47;
        }
        .hero p {
            font-size: 1.3em;
            color: #e0e0e0;
            margin-bottom: 30px;
        }
        .cta-button {
            display: inline-block;
            background-color: #ffd700;
            color: #2c5aa0;
            padding: 15px 36px;
            text-decoration: none;
            border-radius: 30px;
            margin-top: 10px;
            font-weight: bold;
            font-size: 1.1em;
            box-shadow: 0 2px 8px rgba(44,90,160,0.10);
            transition: background 0.2s, color 0.2s;
        }
        .cta-button:hover {
            background-color: #ff6b35;
            color: #fff;
        }
        section {
            padding: 50px 20px;
            max-width: 1200px;
            margin: 0 auto;
        }
        .about {
            background: rgba(255,255,255,0.07);
            border-radius: 18px;
            text-align: center;
            box-shadow: 0 2px 8px rgba(44,90,160,0.07);
        }
        .about h2 {
            font-size: 2.2em;
            color: #ffd700;
            margin-bottom: 18px;
        }
        .about p, .about a {
            color: #f4f4f4;
            font-size: 1.1em;
        }
        .services {
            background: rgba(44,90,160,0.08);
            border-radius: 18px;
        }
        .services h2 {
            text-align: center;
            font-size: 2.2em;
            color: #ffd700;
            margin-bottom: 18px;
        }
        .service-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 22px;
            margin-top: 30px;
        }
        .service-item {
            background: rgba(255,255,255,0.12);
            padding: 22px;
            border-radius: 12px;
            text-align: center;
            box-shadow: 0 2px 8px rgba(44,90,160,0.10);
            color: #fff;
        }
        .service-item h3 {
            color: #ffd700;
            margin-bottom: 10px;
        }
        .videos {
            background: rgba(255,255,255,0.07);
            border-radius: 18px;
        }
        .videos h2 {
            text-align: center;
            font-size: 2.2em;
            color: #ffd700;
            margin-bottom: 18px;
        }
        .video-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 20px;
            margin-top: 30px;
        }
        .video-item iframe {
            width: 100%;
            height: 200px;
            border-radius: 12px;
            box-shadow: 0 2px 8px rgba(44,90,160,0.10);
        }
        .video-item p {
            color: #ffd700;
            font-weight: 500;
            margin-top: 8px;
        }
        .contact {
            background: linear-gradient(90deg, #2c5aa0 60%, #1e2a47 100%);
            color: #fff;
            text-align: center;
            border-radius: 18px;
            box-shadow: 0 2px 8px rgba(44,90,160,0.10);
        }
        .contact h2 {
            font-size: 2.2em;
            color: #ffd700;
            margin-bottom: 18px;
        }
        .contact form {
            max-width: 500px;
            margin: 20px auto;
        }
        .contact input, .contact textarea {
            width: 100%;
            padding: 10px;
            margin: 10px 0;
            border: none;
            border-radius: 5px;
            background: #fff;
            color: #2c5aa0;
            font-size: 1em;
        }
        .contact button {
            background-color: #ffd700;
            color: #2c5aa0;
            padding: 10px 20px;
            border: none;
            border-radius: 30px;
            cursor: pointer;
            font-weight: bold;
            font-size: 1.1em;
            box-shadow: 0 2px 8px rgba(44,90,160,0.10);
            transition: background 0.2s, color 0.2s;
        }
        .contact button:hover {
            background-color: #ff6b35;
            color: #fff;
        }
        footer {
            background: #1e2a47;
            color: #ffd700;
            text-align: center;
            padding: 16px;
            font-size: 1.05em;
            letter-spacing: 1px;
        }
        footer a {
            color: #ffd700;
            text-decoration: underline;
        }
        @media (max-width: 700px) {
            header {
                flex-direction: column;
                text-align: center;
                gap: 10px;
            }
            .header-logo {
                margin-bottom: 10px;
            }
        }
